    "@types/react-lottie": "^1.2.5",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-tooltip": "^3.11.0",
    "@types/react-virtualized": "^9.21.10",
    "@types/semver": "^6.2.1",
    "@types/url-join": "^4.0.0",
    "env-cmd": "^9.0.3",
